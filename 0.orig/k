FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/ABLConditions"
dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $kInlet;

boundaryField
{
    #include    "include/ABLConditions"

    inletOutlet
    {
        type            atmBoundaryLayerInletK;
        #include        "include/ABLConditions"
        value           uniform 0;

    }
    top
    {
        type            atmBoundaryLayerInletK;
        #include        "include/ABLConditions"
        value           uniform 0;
    }
    Buildings
    {
        type            kqRWallFunction;
        value           uniform $kInlet;
    }
    Car
    {
        type            kqRWallFunction;
        value           uniform $kInlet;
    }
    Terrain
    {
        type            kqRWallFunction;
        value           uniform $kInlet;
    }
}


